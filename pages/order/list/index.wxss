.prompt-container {
  width: 100%;
  height: 100%;
  border-top: 1px solid #ccc;
}

/*-------  msg  --------*/
.prompt-msg{
  width:30%;
  display:flex;
  flex-direction:column;
  margin:50px auto;
}

.prompt-msg image {
  width:80px;
  height:80px;
  margin:0 auto;
}
.prompt-msg text {
  font-size:12px;
  line-height:41px;
  color:#ccc;
  text-align:center;
}

.prompt-msg button {
  width:90px;
  height:20px;
  font-size:12px;
  line-height:20px;
  background:#F95C00;
  color:#fff;
  border-radius:3px;
  margin:0 auto;
  padding:0;
}
.scroll-list {
    overflow: hidden;
}

.topbar {
    margin-top: 0;
    background: #fff;
}

.topbar-item {
    color: #333;
    text-align: center;
    padding: 10px 0;
}

.topbar-item.active {
    color: #479de6;
}

.swiper_box {
    width: 100%;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
}